<%- include('./partials/Header.ejs'); %>
<link rel="stylesheet" href="/assets/css/aboutus.css" />
<body>
  <div class="preloader">
    <img
      src="/logos/image3.png"
      height="100px"
      width="100px"
      class="preloader__image"
      alt=""
    />
  </div>
  <!-- /.preloader -->

  <%- include('./partials/Nav.ejs'); %>
  <!-- Banner Section One Start -->
  <section class="banner-one" style="height: 500px">
    <div id="overlay"></div>
    <div
      class="banner-bg-slide"
      data-options='{ "delay": 5000, "slides": [ { "src": "assets/images/properties/slider2.jpg" } ], "transition": "fade", "timer": false, "align": "top", "animation": [ "kenburnsUp", "kenburnsDown", "kenburnsLeft", "kenburnsRight" ] }'
    ></div>
    <!-- /.banner-bg-slide -->
    <div class="container">
      <div class="content-box">
        <div class="top-title">
          <h1>Luxurious Housing Listings</h1>
          <br />
          <br />
          <!-- <h3>making living comfortable and affordable...</h3> -->
        </div>
      </div>
    </div>
  </section>
  <!-- Banner Section One End -->
  <!--Listing One Start-->
  <section class="listing_one_wrap" style="padding-top: 0">
    <div class="container-full-width clearfix">
      <!-- <div class="mapouter"><div class="gmap_canvas"><iframe width="600" height="500" id="gmap_canvas" src="https://maps.google.com/maps?q=Lagos&t=&z=15&ie=UTF8&iwloc=&output=embed" frameborder="0" scrolling="no" marginheight="0" marginwidth="0"></iframe><a href="https://www.whatismyip-address.com"></a><br><style>.mapouter{position:relative;text-align:right;height:500px;width:600px;}</style><a href="https://www.embedgooglemap.net">adding google maps to wordpress</a><style>.gmap_canvas {overflow:hidden;background:none!important;height:500px;width:600px;}</style></div></div> -->
  
      
      <div class="listing_one_content_right">
        <div class="row">
          <div class="col-xl-12">
            <div class="filter">
              <div class="filter_inner_content">
                <div class="left">
                  <div class="left_icon">
                    <a href="#" class="icon-grid active"></a>
                    <!-- <a href="listing-2.html" class="list-icon icon-list"></a> -->
                  </div>
                  <div class="left_text">
                    <h4>Over <%= properties.length %> Results Found</h4>
                  </div>
                </div>
                <div class="right">
                  <div class="shorting">
                    <select class="selectpicker" data-width="100%">
                      <option selected="selected">Default Sorting</option>
                      <option>Default Sorting 1</option>
                      <option>Default Sorting 2</option>
                      <option>Default Sorting 3</option>
                      <option>Default Sorting 4</option>
                    </select>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="listings_page_content">
          <div class="row">
            <%properties.map((property, key) => {%>
            <div class="col-xl-6">
              <!--Listings Page Content Single-->
              <div class="listings_page_content_single">
                <div
                  class="listings_page_content_carousel owl-theme owl-carousel"
                >
                  <div class="listings_page_content_img_box">
                    <div class="listings_page_content_img">
                      <img src="<%= property.image_one %>" alt="" />
                    </div>
                    <div class="listings_page_content_icon">
                      <i class="fa fa-heart"></i>
                    </div>
                    <div class="listings_page_content_btn">
                      <a href="#" class="sale_btn">For Sale</a>
                    </div>
                  </div>
                  <div class="listings_page_content_img_box">
                    <div class="listings_page_content_img">
                      <img src="<%= property.image_two %>" alt="" />
                    </div>
                    <div class="listings_page_content_icon">
                      <i class="fa fa-heart"></i>
                    </div>
                    <div class="listings_page_content_btn">
                      <a href="#" class="sale_btn">For Sale</a>
                    </div>
                  </div>
                  <div class="listings_page_content_img_box">
                    <div class="listings_page_content_img">
                      <img src="<%= property.image_three %>" alt="" />
                    </div>
                    <div class="listings_page_content_icon">
                      <i class="fa fa-heart"></i>
                    </div>
                    <div class="listings_page_content_btn">
                      <a href="#" class="sale_btn">For Sale</a>
                    </div>
                  </div>
                </div>
                <div class="listings_page_bottom_content" style="background: #1b1b1b">
                  <div class="listings_page_bottom_content_top">
                    <h4>
                      <a href="/listing/<%=property.slug %>"
                        ><%= property.title %></a
                      >
                    </h4>
                    <p><%= property.location %></p>
                    <h3>â‚¦<%= property.price %> <span>only</span></h3>
                  </div>
                  <div class="listings_page_bottom_item">
                    <ul class="list-unstyled">
                      <li><span class="icon-bed"></span>4</li>
                      <li><span class="icon-shower"></span>2</li>
                      <li>
                        <span class="icon-square-measument"></span>500 sqft
                      </li>
                    </ul>
                  </div>
                </div>
              </div>
            </div>
            <% })%>
          </div>
          <!-- <div class="row">
                            <div class="col-xl-12">
                                <div class="listings_page_content_load_more_btn">
                                    <a href="#" class="thm-btn">Load More</a>
                                </div>
                            </div>
                        </div> -->
        </div>
      </div>
      <div class="recommended">
        <h1>Recommended</h1>
        <div class="container wow fadeInUp animated">
          <div
            class="card"
            style="background: #1b1b1b; box-shadow: 5px 5px #000;margin-bottom: 30px;"
          >
            <div class="row">
              <div class="col-3">
                <img
                  src="/assets/images/properties/background1.jpeg"
                  
                  style="background-size: cover;"
                  alt=""
                  class="recommended-image"
                />
              </div>
              <div class="col-9  recommended-card">
                <div class="ml-6" style="margin-left: 50px;">
                  <h3>Super Nice Villa</h2>
                <h5>Wuse 2 Abuja</h5>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="container  wow fadeInUp animated">
            <div
              class="card"
              style="background: #1b1b1b; box-shadow: 5px 5px #000;margin-bottom: 30px;"
            >
              <div class="row">
                <div class="col-3">
                  <img
                    src="/assets/images/properties/background1.jpeg"
                    
                    style="background-size: cover;"
                    alt=""
                    class="recommended-image"
                  />
                </div>
                <div class="col-9  recommended-card">
                  <div class="ml-6" style="margin-left: 50px;">
                    <h3>Super Nice Villa</h2>
                  <h5>Wuse 2 Abuja</h5>
                  </div>
                </div>
              </div>
            </div>
          </div>
      </div>
    </div>
  </section>
  <!--Listing One End-->

  <%- include('./partials/Footer.ejs'); %>
</body>
