                                                <div class="form-group row">
                                                    <label class="col-lg-4 col-form-label" for="val-currency">Property Price
                                                        <span class="text-danger">*</span>
                                                    </label>
                                                    <div class="col-lg-6">
                                                        <input required type="number" class="form-control" id="val-currency" name="price" placeholder="price of property in naira">
                                                    </div>
                                                </div>
                                                <div class="form-group row">
                                                    <label class="col-lg-4 col-form-label" for="video">Property Video(Optional)
                                                        <span class="text-danger">*</span>
                                                    </label>
                                                    <div class="col-lg-6">
                                                        <input required type="url" class="form-control" id="video" name="price" placeholder="enter video link">
                                                    </div>
                                                </div>
                                                <div class="form-group row">
                                                    <label class="col-lg-4 col-form-label" for="Bedrooms">Bedrooms
                                                        <span class="text-danger">*</span>
                                                    </label>
                                                    <div class="col-lg-6">
                                                        <input required type="number" class="form-control" id="Bedrooms" value="1" name="bedrooms" >
                                                    </div>
                                                </div>
                                                <div class="form-group row">
                                                    <label class="col-lg-4 col-form-label" for="Bathrooms">Bathrooms
                                                        <span class="text-danger">*</span>
                                                    </label>
                                                    <div class="col-lg-6">
                                                        <input required type="number" class="form-control" id="Bathrooms" value="1" name="bathrooms" >
                                                    </div>
                                                </div>
                                                <div class="form-group row">
                                                    <label class="col-lg-4 col-form-label" for="val-number">Size in Square Feet <span
                                                            class="text-danger">*</span>
                                                    </label>
                                                    <div class="col-lg-6">
                                                        <input required type="number" class="form-control" id="val-number" name="sqft_size" placeholder="size in sqft">
                                                    </div>
                                                </div>
                                             
                                            
                                             
                                            </div>
                                            <div class="col-xl-12" >
                                                <div class="form-group row">
                                                    <label class="col-lg-4 col-form-label"  for="val-confirm-password">Property amenities <span
                                                            class="text-danger">*</span>
                                                    </label>
                                                    <div class="col-lg-6">
														<select class="multi-select" placeholder = "select the amenities" name="amenities[]" multiple="multiple">
															<option value="AL">Alabama</option>
															<option value="WY">Wyoming</option>
															<option value="UI">dlf</option>
														</select> </div>
                                                </div>
                                                <div class="form-group row">
                                                    <label class="col-lg-12 col-form-label" for="val-suggestions">Property Content <span
                                                            class="text-danger">*</span>
                                                    </label>
                                                    <div class="col-lg-12">
                                                        <textarea required class="form-control" id="val-suggestions" name="val-suggestions" rows="10" placeholder="a short description about the property"></textarea>
                                                    </div>
                                                </div>


                                                <div class="form-group row">
                                                    <center>
