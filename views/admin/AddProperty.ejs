
<%- include('./partials/Header.ejs'); %>
<%- include('./partials/Sidebar.ejs'); %>

        <!--**********************************
            Content body start
        ***********************************-->
        <div class="content-body">
            <div class="container-fluid">

                <div class="page-titles">
					<ol class="breadcrumb">
						<li class="breadcrumb-item"><a href="javascript:void(0)">Home</a></li>
						<li class="breadcrumb-item active"><a href="javascript:void(0)">Wizard</a></li>
					</ol>
                </div>
                <!-- row -->
                <div class="row">
                    <div class="col-xl-12 col-xxl-12">
                        <div class="card">
                            <div class="card-header">
                                <h4 class="card-title">Form step</h4>
                            </div>
                            <div class="card-body">
								<div id="smartwizard" class="form-wizard order-create">
									<ul class="nav nav-wizard">
										<li><a class="nav-link" href="#wizard_Service"> 
											<span>1</span> 
										</a></li>
										<li><a class="nav-link" href="#wizard_Time">
											<span>2</span>
										</a></li>
										<li><a class="nav-link" href="#wizard_Details">
											<span>3</span>
										</a></li>
										<li><a class="nav-link" href="#wizard_Payment">
											<span>4</span>
										</a></li>
									</ul>
									<div class="tab-content">
										<div id="wizard_Service" class="tab-pane" role="tabpanel">
											<div class="row">
												<div class="col-lg-6 mb-2">
													<div class="form-group">
														<label class="text-label">Property Title*</label>
														<input type="text" name="title" class="form-control" placeholder="name of property" required>
													</div>
												</div>
												<div class="col-lg-6 mb-2">
													<div class="form-group">
														<label class="text-label">Property Address*</label>
														<input type="address" name="location" class="form-control" placeholder="Property address" required>
													</div>
												</div>
												<div class="col-lg-12 mb-3">
													<div class="form-group">
														<label class="text-label">Property Content*</label>
														<div class="card-body">
															<div class="summernote" name = "content"></div>
														</div>
													</div>
												</div>
											</div>
										</div>
										<div id="wizard_Time" class="tab-pane" role="tabpanel">
											<div class="row">
												<div class="col-lg-6 mb-2">
													<div class="form-group">
														<label class="text-label">Category*</label>
														<select id="single-select">
															<option value="AL">Alabama</option>
															<option value="WY">Wyoming</option>
														</select>	</div>
												</div>
												<div class="col-lg-6 mb-2">
													<div class="form-group">
														<label class="text-label">Amenities</label>
														<select class="multi-select" name="amenities[]" required multiple="multiple">
															<option value="AL">Alabama</option>
															<option value="WY">Wyoming</option>
															<option value="UI">dlf</option>
														</select>	</div>
												</div>
											
											</div>
										</div>
										<div id="wizard_Details" class="tab-pane" role="tabpanel">
											<div class="row">
												<div class="col-sm-4 mb-2">
													<h4>Bedrooms *</h4>
												</div>
												<div class="col-6 col-sm-4 mb-2">
													<div class="form-group">
														<input class="form-control" value="1" type="number" name="bedrooms" id="input1">
													</div>
												</div>
											
											</div>
											<div class="row">
												<div class="col-sm-4 mb-2">
													<h4>Bathrooms *</h4>
												</div>
												<div class="col-6 col-sm-4 mb-2">
													<div class="form-group">
														<input class="form-control" value="1" type="number" name="bathrooms" id="input3">
													</div>
												</div>
												
											</div>
											<div class="row">
												<div class="col-sm-4 mb-2">
													<h4>Size in Square Feet *</h4>
												</div>
												<div class="col-6 col-sm-4 mb-2">
													<div class="form-group">
														<input class="form-control" value="1" type="number" name="sqft_size" id="input5">
													</div>
												</div>
											
											</div>
										
											<div class="row">
												<div class="col-sm-4 mb-2">
													<h4>Video of Property (optional)</h4>
												</div>
												<div class="col-12 col-sm-8 mb-2">
													<div class="form-group">
														<input class="form-control" placeholder="video url" type="url" name="video" id="input9">
													</div>
												</div>
											
											</div>
										</div>
										<div id="wizard_Payment" class="tab-pane" role="tabpanel">
											<center>	<h4>Property images</h4></center>
										
											<div class="row emial-setup">
											
												<div class="col-lg-6 col-sm-12">
													<div class="form-group">
												
															<input type="file" class="custom-file-input">
															<label class="custom-file-label">Choose first image</label>
													
													</div>
												</div>
												<div class="col-lg-6 col-sm-12 col-xs-12">
													<div class="form-group">
														<input type="file" class="custom-file-input">
														<label class="custom-file-label">Choose Second image</label>
													</div>
												</div>
												<div class="col-lg-6 col-sm-12">
													<div class="form-group">
														<input type="file" class="custom-file-input">
														<label class="custom-file-label">Choose third image</label>
													</div>
												</div>
												<div class="col-lg-6 col-sm-12">
													<div class="form-group">
														<input type="file" class="custom-file-input">
														<label class="custom-file-label">Choose last image</label>
													</div>
												</div>
											</div>

											<div class="row">
												<div class="col-12">
													<div class="skip-email text-center">
													
														<button type="submit" class="btn btn-primary mb-2">Confirm identity</button>
                                  
													</div>
												</div>
											</div>
										</div>
									</div>
								</div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--**********************************
            Content body end
        ***********************************-->


