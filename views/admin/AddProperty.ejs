    <script src="/dashboard/vendor/global/global.min.js"></script>
	<script src="/dashboard/vendor/bootstrap-select/dist/js/bootstrap-select.min.js"></script>
	

    <!-- Summernote -->
    <script src="/dashboard/vendor/summernote/js/summernote.min.js"></script>
    <!-- Summernote init -->
    <script src="/dashboard/js/plugins-init/summernote-init.js"></script>

    <script src="/dashboard/js/custom.min.js"></script>
	<script src="/dashboard/js/deznav-init.js"></script>
	
	
    


    <script src="/dashboard/vendor/jquery-steps/build/jquery.steps.min.js"></script>
    <script src="/dashboard/vendor/jquery-validation/jquery.validate.min.js"></script>
    <!-- Form validate init -->
    <script src="/dashboard/js/plugins-init/jquery.validate-init.js"></script>

	<script src="/dashboard/vendor/select2/js/select2.full.min.js"></script>
    <script src="/dashboard/js/plugins-init/select2-init.js"></script>

	


   <!-- Form Steps -->
