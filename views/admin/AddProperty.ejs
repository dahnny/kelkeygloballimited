                                                    <div class="col-lg-6">
                                                        <input type="address" class="form-control" required id="val-email" name="location" placeholder="Your property location..">
                                                    </div>
                                                </div>
                                                <div class="form-group row">
                                                    <label class="col-lg-4 col-form-label" for="val-password">Category
                                                        <span class="text-danger">*</span>
                                                    </label>
