    <script src="/dashboard/js/custom.min.js"></script>
	<script src="/dashboard/js/deznav-init.js"></script>
	
	
    


    <script src="/dashboard/vendor/jquery-steps/build/jquery.steps.min.js"></script>
    <script src="/dashboard/vendor/jquery-validation/jquery.validate.min.js"></script>
    <!-- Form validate init -->
    <script src="/dashboard/js/plugins-init/jquery.validate-init.js"></script>

	


   <!-- Form Steps -->
	<script src="/dashboard/vendor/jquery-smartwizard/dist/js/jquery.smartWizard.js"></script>
	
	<script>
		$(document).ready(function(){
			// SmartWizard initialize
			$('#smartwizard').smartWizard(); 
		});
	</script>

</body>

